# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# ODPT API (Public Transport Open Data Center)
# Register at: https://developer.odpt.org/
ODPT_API_KEY_METRO=your-tokyo-metro-api-key
ODPT_API_KEY_JR_EAST=your-jr-east-challenge-key
ODPT_API_KEY_PUBLIC=optional-public-api-key

# Mapbox (Optional, if using Mapbox tiles)
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token

# AI Backend (n8n)
N8N_WEBHOOK_URL=https://n8n-k7m3694.zeabur.app/webhook/lutagu-chat
N8N_WEBHOOK_SECRET=optional_secret_token

# LINE Messaging API (For Notifications)
LINE_CHANNEL_ACCESS_TOKEN=your-line-access-token
LINE_CHANNEL_SECRET=your-line-channel-secret

# Activity Logging
ACTIVITY_LOGGING_ENABLED=true
ACTIVITY_HASH_SALT=replace-with-random-secret

# Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Member PII Encryption (32-byte key in base64)
PII_ENCRYPTION_KEY_BASE64=replace-with-32-byte-base64

# Security Monitoring
SECURITY_ALERT_WEBHOOK_URL=

# Rate limiting
RATE_LIMIT_ENABLED=true

# AI/ML Configuration

# Zeabur AI Hub - Main Key (for Gemini 3 Flash / 2.5 Flash)
ZEABUR_API_KEY=your-zeabur-key-for-gemini

# Zeabur AI Hub - DeepSeek Key (lutagu-mvp)
DEEPSEEK_API_KEY=your-zeabur-lutagu-mvp-key

# MiniMax API (Fallback)
MINIMAX_API_KEY=your-minimax-key

# Voyage AI (Primary Embedding)
VOYAGE_API_KEY=your-voyage-api-key

# Google AI API (Free - Fallback Embedding)
GEMINI_API_KEY=your-google-ai-key

# Embedding Provider (voyage or gemini)
EMBEDDING_PROVIDER=voyage

# Cache TTL (in milliseconds)
CACHE_TTL_MS=300000

# Redis Cache (Optional - Upstash)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Service Integration (Rust & Go Agents)
L4_ROUTING_API_URL=http://localhost:8082
L2_STATUS_API_URL=http://localhost:8083
CHAT_API_URL=http://localhost:8080/api

# AI Chat Proxy (use either ADK_SERVICE_URL or CHAT_API_URL)
# ADK_SERVICE_URL is preferred for the Next.js proxy layer
# Production: https://chat-api-y6r3wpax5q-an.a.run.app/agent/chat
# Development: http://localhost:8080/agent/chat
ADK_SERVICE_URL=https://chat-api-y6r3wpax5q-an.a.run.app/agent/chat


